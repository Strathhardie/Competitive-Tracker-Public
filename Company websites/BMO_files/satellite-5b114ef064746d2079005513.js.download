function getDSTLabel(text, anaMappingObj) {
    for (key in anaMappingObj.bmo_public) {
        if (text === decodeURIComponent(anaMappingObj.bmo_public[key])) {
            return anaMappingObj.dst[key];
        }
    }
    return "no translator match found";
}

document.addEventListener('DOMContentLoaded', function(evt){

    var dataApplyNowEls = Array.prototype.slice.call(document.querySelectorAll('[data-ana-apply-now]'));

    dataApplyNowEls.forEach(function(el) {
        var dataAnaApplyNowObj = JSON.parse(el.dataset.anaApplyNow);
        dataAnaApplyNowObj.anaProdName = getDSTLabel(dataAnaApplyNowObj.productName, anaEDBs[BMOINFO.language]);
        el.dataset.anaApplyNow = JSON.stringify(dataAnaApplyNowObj);
    });

});
